100 REM COMPUTE PRIME NUMBERS USING SIEVE OF ERATOSTHENES 
105 LET L = 1000
110 DIM N(L)
120 FOR I = 1 TO L-1
130 LET N(I) = I
140 NEXT I
150 LET P = 2
160 PRINT P; " ";
170 FOR I = P TO L-1 STEP P
180 LET N(I) = 0
190 NEXT I
200 LET P = P + 1
210 IF P = L THEN 230
220 IF N(P) <> 0 THEN 160
225 GOTO 200
230 END
